{% extends "main.twig" %}


{% block headers %}
	<link href="/css/play.css" rel="stylesheet" type="text/css"/>
{% endblock %}


{% block menu %}
	{{ include('menu.twig') }}
{% endblock %}

{% block interactivemenus %}
	{{include('interactivemenus/loadfile.twig')}}
{% endblock %}
{% block content %}

	<div id="main_page" class="container-fluid row" playlistid="{{playlistid}}">

		<div class="col-sm-3">

			<div class="sidenavbar" linkstyle="btn btn-primary">
				<ul class="">
					<div class="input-group flex-nowrap"></div>
				</ul>
				<ul class="sidenavbarlist"></ul>

			</div>
		</div>
		<div class="col-sm-9">
			<h1>{{playlistname}}</h1>

			<div id="play" class="row">
				<div class="player circle">
					<div class="buffering circle animated"></div>
					<div class="triangle"></div>
				</div>
				{# {{dump(tracks)}} #}

			</div>
		</div>


	</div>

{% endblock %}

{% block javascripts %}
	<script type="text/javascript">
		$(document).ready(function () {
SideNavBar.init();
});
	</script>
	<script src="/js/play.js" type="text/javascript"></script>
{% endblock %}
